 <div id="supporters" class="logos-area">
    <div class="main-color">
      Apoio
    </div>
    <% @supporters.each do |supporter| %>
    <div class="supporter">
      <a target="_blank" href="<%= supporter.website_url %>">
        <img src="<%= supporter.image_url %>" alt="<%= supporter.name %>" />
      </a>
    </div>
    <% end %>
  </div>


